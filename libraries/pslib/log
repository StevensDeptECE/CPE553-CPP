/drawquad
{
  moveto
  lineto
  lineto
  lineto
  closepath
  stroke
} bind def

/fillquad
{
  mul cvi setcolor
  moveto
  lineto
  lineto
  lineto
  fill
} bind def


/plotmaskedmatrix {
  5 dict begin
  /grid exch def

  0 setlinewidth
  0 1 grid /jmax get 1 sub  {
    /j exch def
    grid /mask get j get /row exch def
    0 1 grid /imax get 1 sub {
      /i exch def
      row i get 48 ne {
	i j 1 1 rectstroke
      } if
    } for
  } for
  end
} bind def

/numbermaskedmatrix {
  8 dict begin
  /varname exch def
  /grid exch def
  /valindex 0 def
  0 setlinewidth
  0 1 grid /jmax get 1 sub  {
    /j exch def
    grid /mask get j get /row exch def
    0 1 grid /imax get 1 sub {
      /i exch def
      row i get 48 ne {
	i j 1 1 rectstroke
	i j moveto
	grid varname get valindex get (        ) cvs 0 2 getinterval show
	/valindex valindex 1 add def
      } if
    } for
  } for
  end
} bind def


/fillmaskedmatrix {
  8 dict begin
  /varname exch def
  /grid exch def
  /colorscale 1 100 div 255 mul def
  /valindex 0 def
  0 setlinewidth
  0 1 grid /jmax get 1 sub  {
    /j exch def
    grid /mask get j get /row exch def
    0 1 grid /imax get 1 sub {
      /i exch def
      row i get 48 ne {
	grid varname get valindex get colorscale mul cvi setcolor
	i j 1 1 rectfill

	/valindex valindex 1 add def
      } if
    } for
  } for
  end
} bind def


